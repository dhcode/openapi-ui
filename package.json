{
  "name": "openapi-ui",
  "version": "0.0.1",
  "description": "UI to display Swagger and OpenAPI RESTful API specifications.",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "start": "ng serve",
    "start:es5": "ng serve --configuration es5",
    "build": "ng build",
    "test": "ng test",
    "test:lib": "ng test openapi-viewer",
    "test:lib:ci": "ng test openapi-viewer --watch=false --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "keywords": [
    "OpenAPI",
    "JavaScript",
    "TypeScript",
    "Angular",
    "JSON",
    "viewer",
    "API Console",
    "Swagger",
    "Angular"
  ],
  "author": "Dominik Herbst",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhcode/openapi-ui.git"
  },
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/cdk": "^8.1.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "@schuchard/prettier": "^3.1.0",
    "ajv": "^6.10.2",
    "brace": "^0.11.1",
    "core-js": "^3.2.1",
    "ngx-ace-wrapper": "^8.0.0",
    "ngx-markdown": "^8.1.0",
    "openapi3-ts": "^1.3.0",
    "rxjs": "~6.5.2",
    "swagger-client": "^3.9.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.0",
    "@angular-devkit/build-ng-packagr": "^0.803.0",
    "@angular/cli": "^8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/json-schema": "^7.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.5.0",
    "prettier": "1.18.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.5.3"
  }
}
